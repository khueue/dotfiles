{
    "[markdown]": {
        "editor.quickSuggestions": false,
        "editor.wordWrap": "off"
    },
    "[javascript]": {
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorBlinking": "solid",
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'Code New Roman', 'Borg Sans Mono', Menlo, 'Courier New', monospace",
    "editor.fontSize": 18,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80
    ],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "explorer.sortOrder": "type",
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "indentRainbow.updateDelay": 250,
    "powermode.enableExplosions": true,
    "powermode.enableShake": false,
    "powermode.enableStatusBarComboCounter": false,
    "powermode.enableStatusBarComboTimer": false,
    "powermode.explosionOffset": 0.4,
    "powermode.explosionSize": 8,
    "powermode.presets": "particles",
    "prolog.executablePath": "/dev/null",
    "prolog.format.enabled": false,
    "prolog.format.insertSpaces": false,
    "prolog.format.tabSize": 8,
    "prolog.linter.delay": 500000000,
    "prolog.linter.run": "never",
    "scm.diffDecorationsGutterWidth": 5,
    "search.location": "panel",
    "search.smartCase": false,
    "telemetry.enableTelemetry": false,
    "vim.highlightedyank.duration": 30,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.sneak": true,
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<shift+tab>"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                "<tab>"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        }
    ],
    "window.closeWhenEmpty": false,
    "window.newWindowDimensions": "inherit",
    "window.openFilesInNewWindow": "on",
    "window.openFoldersInNewWindow": "on",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.showTabs": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 22,
    "editor.find.addExtraSpaceOnTop": false,
    "prettier.singleQuote": true,
    "prettier.trailingComma": "all",
    "workbench.colorTheme": "Oblivion",
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    }
}
