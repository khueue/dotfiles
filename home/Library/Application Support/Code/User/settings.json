{
	"[javascript]": {
		"editor.formatOnSave": false
	},
	"[markdown]": {
		"editor.quickSuggestions": false,
		"editor.wordWrap": "off"
	},
	"[typescript]": {
		"editor.formatOnSave": false
	},
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.cursorBlinking": "solid",
	"editor.cursorSurroundingLines": 2,
	"editor.dragAndDrop": false,
	"editor.find.addExtraSpaceOnTop": false,
	"editor.fontFamily": "JuliaMono, Menlo, 'Code New Roman', 'Borg Sans Mono', Menlo, 'Courier New', monospace",
	"editor.fontSize": 18,
	"editor.insertSpaces": false,
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "boundary",
	"editor.rulers": [
		80
	],
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.openEditors.visible": 0,
	"explorer.sortOrder": "type",
	"extensions.ignoreRecommendations": true,
	"files.autoSave": "onFocusChange",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"scm.diffDecorationsGutterWidth": 5,
	"search.smartCase": false,
	"telemetry.enableTelemetry": false,
	"vetur.completion.autoImport": false,
	"vim.highlightedyank.duration": 30,
	"vim.highlightedyank.enable": true,
	"vim.hlsearch": true,
	"vim.sneak": true,
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<shift+tab>"
			],
			"commands": [
				"editor.action.outdentLines"
			]
		},
		{
			"before": [
				"<tab>"
			],
			"commands": [
				"editor.action.indentLines"
			]
		}
	],
	"window.closeWhenEmpty": false,
	"window.newWindowDimensions": "inherit",
	"window.openFilesInNewWindow": "on",
	"window.openFoldersInNewWindow": "on",
	"window.zoomLevel": 0,
	"workbench.activityBar.visible": false,
	"workbench.colorCustomizations": {
		"statusBar.background": "#222"
	},
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.editor.showTabs": true,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.tree.indent": 22,
	"editor.copyWithSyntaxHighlighting": false,
	"timeline.excludeSources": [
		"git-history"
	],
	"vetur.useWorkspaceDependencies": true,
	"explorer.compactFolders": false,
	"javascript.updateImportsOnFileMove.enabled": "never",
	"workbench.colorTheme": "Slime Solid",
	"breadcrumbs.enabled": false,
	"editor.largeFileOptimizations": false,
	"typescript.updateImportsOnFileMove.enabled": "never",
	"vetur.format.options.useTabs": true
}
