{
	"[javascript]": {
		"editor.formatOnSave": false
	},
	"[markdown]": {
		"editor.quickSuggestions": false,
		"editor.wordWrap": "off"
	},
	"[typescript]": {
		"editor.formatOnSave": false
	},
	"breadcrumbs.enabled": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.copyWithSyntaxHighlighting": false,
	"editor.cursorBlinking": "solid",
	"editor.cursorSurroundingLines": 2,
	"editor.dragAndDrop": false,
	"editor.find.addExtraSpaceOnTop": false,
	"editor.fontFamily": "Menlo, JuliaMono,  'Code New Roman', 'Borg Sans Mono', Menlo, 'Courier New', monospace",
	"editor.fontSize": 18,
	"editor.insertSpaces": false,
	"editor.largeFileOptimizations": false,
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "boundary",
	"editor.rulers": [
		80
	],
	"explorer.compactFolders": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.openEditors.visible": 0,
	"explorer.sortOrder": "type",
	"extensions.ignoreRecommendations": true,
	"files.autoSave": "onFocusChange",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"javascript.updateImportsOnFileMove.enabled": "never",
	"scm.diffDecorationsGutterWidth": 5,
	"search.smartCase": false,
	"telemetry.enableTelemetry": false,
	"timeline.excludeSources": [
		"git-history"
	],
	"typescript.updateImportsOnFileMove.enabled": "never",
	"vetur.format.enable": true,
	"vetur.completion.autoImport": false,
	"vetur.format.options.useTabs": true,
	"vetur.useWorkspaceDependencies": true,
	"vim.highlightedyank.duration": 30,
	"vim.highlightedyank.enable": true,
	"vim.hlsearch": true,
	"vim.sneak": true,
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<shift+tab>"
			],
			"commands": [
				"editor.action.outdentLines"
			]
		},
		{
			"before": [
				"<tab>"
			],
			"commands": [
				"editor.action.indentLines"
			]
		}
	],
	"window.closeWhenEmpty": false,
	"window.newWindowDimensions": "inherit",
	"window.openFilesInNewWindow": "on",
	"window.openFoldersInNewWindow": "on",
	"workbench.activityBar.visible": false,
	"workbench.colorCustomizations": {
		"statusBar.background": "#222"
	},
	"workbench.colorTheme": "Nord",
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.editor.showTabs": true,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.tree.indent": 22,
	"svelte.plugin.svelte.format.config.svelteStrictMode": true,
	"javascript.format.enable": false,
	"html.format.enable": false,
	"emmet.triggerExpansionOnTab": true,
	"python.showStartPage": false,
	"python.languageServer": "Pylance",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb"
	},
	"editor.cursorStyle": "line",
	"editor.lineNumbers": "on",
	"editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
	"editor.wordWrap": "off",
	"workbench.editor.untitled.hint": "hidden",
	"editor.inlineSuggest.enabled": true,
	"github.copilot.enable": {
		"*": false,
		"yaml": false,
		"plaintext": false,
		"markdown": false
	}
}
